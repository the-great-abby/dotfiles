<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.abby.gtd.weekly</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/abby/code/dotfiles/bin/gtd-weekly-reminder</string>
    </array>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Weekday</key>
        <integer>0</integer>
        <key>Hour</key>
        <integer>9</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>
    <key>RunAtLoad</key>
    <false/>
    <key>StandardOutPath</key>
    <string>/tmp/gtd-weekly-reminder.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/gtd-weekly-reminder.error.log</string>
</dict>
</plist>

